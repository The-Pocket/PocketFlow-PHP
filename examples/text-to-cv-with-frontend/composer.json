{
    "name": "weise25/pocketflow-php",
    "version": "0.1",
    "description": "A PHP port of the minimalist LLM framework PocketFlow.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PocketFlow\\": "src/"
        },
        "files": [
            "src/PocketFlow.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PocketFlow\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "react/async": "^4.0",
        "react/promise-timer": "^1.9",
        "vlucas/phpdotenv": "^5.6",
        "dompdf/dompdf": "^3.1",
        "openai-php/client": "^0.14.0",
        "guzzlehttp/guzzle": "^7.9",
        "php-http/guzzle7-adapter": "^1.1",
        "symfony/yaml": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
